<div id="<%= dom_id cocktail %>">
  <p>
    <strong>Name:</strong>
    <%= cocktail.name %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= cocktail.image %>
  </p>

  <p>
    <strong>Ingredients:</strong>
    <%= cocktail.ingredients %>
  </p>

  <p>
    <strong>Directions:</strong>
    <%= cocktail.directions %>
  </p>

  <p>
    <strong>User:</strong>
    <%= cocktail.user_id %>
  </p>

</div>
